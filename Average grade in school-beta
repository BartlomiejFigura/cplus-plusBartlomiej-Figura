//program to calculate your average grade from 10 subjects
#include <iostream>
using namespace std;
int main()
{
	int arr[10], sum=0, i=0;
	double average=0.0;
	
	cout<<"Write grades from your subjects (10)"<<endl;
	
	for( int i=0; i<10; ++i){
        cin>>arr[i];
        
	    	
	}
	//cout<<"domekdomek"<<endl;	
	for(int i=0; i<10; ++i){
	    //cout<<arr[i];
	    sum+=arr[i];	
	}
	//cout<<sum<<endl;

    average=sum/10.0;
	cout<<"Average grade is "<<average<<endl;
return 0;		
